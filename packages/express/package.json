{
  "name": "@auth/express",
  "description": "Authentication for Express.",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "index.*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.js && node scripts/postbuild",
    "patch": "npm version patch --no-git-tag-version",
    "clean": "rm -rf client.* index.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@auth/core": "workspace:*",
    "@types/cookie": "0.5.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.7.14",
    "@types/set-cookie-parser": "^2.4.2",
    "cookie": "^0.5.0",
    "express": "^4.18.2",
    "next-auth": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@auth/core": "~0.2.2 || ^0.2.2",
    "cookie": "^0.5.0",
    "express": "^4.18.2"
  },
  "dependencies": {
    "set-cookie-parser": "^2.5.1"
  },
  "keywords": [
    "Express",
    "Express.js",
    "Auth"
  ],
  "author": "Ivo Ilic",
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC"
}
